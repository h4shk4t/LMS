{% extends "templates/partials/base.twig" %}

{% block title %} LMS - Issues {% endblock %}

{% block body %}
<div class="results">
    <table class="table">
        <thead>
            <tr>
                <th scope="col">User ID</th>
                <th scope="col">Name</th>
                <th scope="col">Enrollment Number</th>
                <th scope="col">Book ID</th>
            </tr>
        </thead>
        {% for user in users %}
            <tr>
                <th>{{ user.USERID }}</th>
                <td>{{ user.NAME }}</td>
                <td>{{ user.ENO }}</td>
                <td>{{ user.BOOKID }}</td>
            </tr>
        {% endfor %}
    </table>
</div>
{% endblock %}